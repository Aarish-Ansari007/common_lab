<script type="text/javascript">
 $(document).ready(function() {
	 setInterval(function() {       	
	 $.ajax({
            	    url: "get-dashboard-record",
            	    type: 'GET',
            	    datatype:"json",
            	    contentType: "application/json; charset=utf-8",
            	    success: function (responce)
            	    {
            	    	$("#regStaff").html(responce.totalStaffCount);
            	    	$("#regLab").html(responce.totalLabCount);
            	    	$("#regTest").html(responce.tatalTestCount);
            	    	//$("#regPatient").html(responce.totalPatientCount);
            	    	$("#totalRev").html(responce.totalRevenue);
            	    	$("#totalRecRev").html(responce.totalReceivedRevenue);
            	    	$("#totalPenRev").html(responce.totalPeningRevenue);
            	    	$("#weeklyRev").html(responce.weeklyRevenue);
            	    	$("#weeklyRecRev").html(responce.weeklyReceivedRevenue);
            	    	$("#weeklyPenRev").html(responce.weeklyPendingRevenue);
            	    	$("#dailyRev").html(responce.dailyRevenue);
            	    	$("#dailyRecRev").html(responce.dailyReceivedRevenue);
            	    	$("#dailyPenRev").html(responce.dailyPendingRevenue);
            	    	
					}
            	}); 
	 }, 1000);
            	});
 
 </script>
